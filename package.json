    "dependencies": {
    },
    "devDependencies": {
      "forge-std": "github:foundry-rs/forge-std#v1.8.2",
      "prettier": "^3.3.2",
      "solhint": "^5.0.1"
    },
    "keywords": [
      "blockchain",
      "ethereum",
      "forge",
      "foundry",
      "smart-contracts",
      "solidity",
